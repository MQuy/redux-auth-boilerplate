{
  "name": "redux-boilerplate",
  "main": "main.js",
  "version": "1.0.0",
  "description": "Sample for redux",
  "scripts": {
    "start": "better-npm-run start",
    "client": "babel-node ./cli/client.js",
    "server": "babel-node ./cli/server.js",
    "build": "better-npm-run build",
    "lint": "better-npm-run lint",
    "clean": "rimraf publish",
    "test": "better-npm-run test",
    "pack": "babel-node ./cli/electron.js"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --hot",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --progress --config ./cli/webpack.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start ./cli/webpack.test.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "lint": {
      "command": "babel-node ./node_modules/stylelint/dist/cli.js src/**/*.scss && babel-node ./node_modules/eslint/bin/eslint.js src/**/*.js"
    }
  },
  "author": "MQuy",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
